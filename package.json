{
  "name": "confify",
  "version": "0.0.6",
  "description": "Simplified nconf. Works in the browser.",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cellvia/node-confify.git"
  },
  "keywords": [
    "conf",
    "nconf",
    "configuration",
    "browserify",
    "browser",
    "browserify-transform"
  ],
  "author": "Brandon Selway",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cellvia/node-confify/issues"
  },
  "homepage": "https://github.com/cellvia/node-confify",
  "devDependencies": {
    "browserify": "~3.33.0",
    "tape": "~2.12.0"
  },
  "confify": "./test/global.json",
  "browserify": {
    "transform": [
      "transform.js"
    ]
  },
  "browser": {
    "confify": "./browser.js",
    "index.js": "./browser.js"
  },
  "testling": {
    "files": "test/browser/*.js",
    "browsers": [
      "ie/8..latest",
      "firefox/21..latest",
      "firefox/nightly",
      "chrome/26..latest",
      "chrome/canary",
      "opera/12..latest",
      "opera/next",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  },
  "dependencies": {
    "through": "~2.3.4",
    "falafel": "~0.3.1",
    "escodegen": "~1.3.0",
    "mothership": "git+http://github.com/cellvia/mothership.git"
  }
}
